<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--layout definitions-->
    <declare-styleable name="DarkenRoundedBackgroundLayout">

        <attr name="round_number" format="float"/>
        <attr name="background_alpha" format="float"/>
        <attr name="round_back_color" format="color"/>
        <attr name="round_type" format="enum">
            <enum name="TOP" value="0"/>
            <enum name="ALL" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="MotionRoundedBitmapLayout">
        <attr name="blur" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ConstraintLayoutView">
        <attr name="layout_weight_width" format="integer"/>
        <attr name="layout_weight_height" format="integer"/>
    </declare-styleable>

<!--    <declare-styleable name="ImageView"></declare-styleable>-->

    <declare-styleable name="MotionRadiusLayout">
        <attr name="background_color" format="integer"/>
        <attr name="radius_value" format="dimension"/>
        <attr name="radius_type" format="enum">
            <enum name="TOP" value="0"/>
            <enum name="ALL" value="1"/>
            <enum name="BOTTOM" value="1"/>
        </attr>
    </declare-styleable>








